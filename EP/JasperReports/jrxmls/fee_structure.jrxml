	<queryString language="SQL">
		<![CDATA[WITH fee_components AS (
    SELECT
        c.description AS component_description,
        c.fee_status AS component_fee_status,
        t.term_name,
        t.id AS term_id,
        CAST(c.amount AS DECIMAL(10,2)) AS component_amount,
        fs.name AS fee_structure_name,
        fs.total_amount AS fee_structure_total,
        fs.year AS fee_structure_year,
        g.name AS grade_name,
        g.id AS grade_id
    FROM fee_structure fs
    JOIN component c ON fs.id = c.fee_structure_id
    JOIN term t ON t.id = c.term_id
    JOIN grade g ON g.id = fs.grade_id
    WHERE fs.grade_id = $P{gradeId}
      AND fs.year = $P{year}
),
distinct_components AS (
    SELECT DISTINCT
        component_description,
        component_fee_status
    FROM fee_components
),
component_amounts AS (
    SELECT
        dc.component_description,
        dc.component_fee_status,
        MAX(CASE WHEN fc1.term_name = 'Term 1' THEN fc1.component_amount END) AS term1_amount,
        MAX(CASE WHEN fc2.term_name = 'Term 2' THEN fc2.component_amount END) AS term2_amount,
        MAX(CASE WHEN fc3.term_name = 'Term 3' THEN fc3.component_amount END) AS term3_amount,
        MAX(fc1.fee_structure_name) AS fee_structure_name,
        MAX(fc1.fee_structure_total) AS fee_structure_total,
        MAX(fc1.fee_structure_year) AS fee_structure_year,
        MAX(fc1.grade_name) AS grade_name
    FROM distinct_components dc
    LEFT JOIN fee_components fc1 ON dc.component_description = fc1.component_description AND fc1.term_name = 'Term 1'
    LEFT JOIN fee_components fc2 ON dc.component_description = fc2.component_description AND fc2.term_name = 'Term 2'
    LEFT JOIN fee_components fc3 ON dc.component_description = fc3.component_description AND fc3.term_name = 'Term 3'
    GROUP BY
        dc.component_description,
        dc.component_fee_status
),
term_totals AS (
    SELECT
        SUM(term1_amount) AS sum_term1,
        SUM(term2_amount) AS sum_term2,
        SUM(term3_amount) AS sum_term3
    FROM component_amounts
)

SELECT *
FROM (
    SELECT
        component_description,
        component_fee_status,
        term1_amount,
        term2_amount,
        term3_amount,
        fee_structure_name,
        fee_structure_total,
        fee_structure_year,
        grade_name,
        NULL AS is_total_row,
        NULL AS total_label
    FROM component_amounts

    UNION ALL

    SELECT
        NULL,
        NULL,
        sum_term1,
        sum_term2,
        sum_term3,
        NULL,
        NULL,
        NULL,
        NULL,
        1,
        'TERM TOTALS'
    FROM term_totals

    UNION ALL

    SELECT
        NULL,
        NULL,
        NULL,
        NULL,
        (sum_term1 + sum_term2 + sum_term3),
        NULL,
        NULL,
        NULL,
        NULL,
        2,
        'ANNUAL TOTAL'
    FROM term_totals
) AS result
ORDER BY
    CASE WHEN is_total_row IS NULL THEN 0 ELSE is_total_row END,
    component_description;]]>

    <queryString language="SQL">
        <![CDATA[WITH fee_data AS (
    SELECT
        fs.id AS fee_structure_id,
        fs.name AS fee_structure_name,
        fs.total_amount AS fee_structure_total,
        fs.year AS fee_structure_year,
        c.id AS component_id,
        c.amount AS component_amount,
        c.deadline AS component_deadline,
        c.description AS component_description,
        c.fee_status AS component_fee_status,
        t.id AS term_id,
        t.term_name AS term_name,
        g.id AS grade_id,
        g.name AS grade_name,
        s.id AS student_id,
        s.first_name AS student_first_name,
        s.last_name AS student_last_name,
        fso.id AS fso_id,
        fso.created_by AS fso_created_by,
        fso.parent_id AS fso_parent_id,
        fso.route_id AS fso_route_id,
        -- Transport price information
        trp.id AS transport_price_id,
        trp.route AS transport_route,
        trp.price AS transport_price,
        trp.routes_options AS transport_route_option,
        NULL AS is_total_row,
        NULL AS total_label
    FROM fee_structure fs
    JOIN component c ON fs.id = c.fee_structure_id
    JOIN term t ON t.id = c.term_id
    JOIN grade g ON g.id = fs.grade_id
    JOIN student s ON s.grade_id = g.id
    LEFT JOIN fee_structure_selected_optionals fso ON fso.component_id = c.id
        AND fso.student_id = s.id
        AND fso.grade_id = g.id
        AND fso.term_id = t.id
        AND fso.fee_structure_id = fs.id
    -- Join with TransportRoutesPrice table
    LEFT JOIN transport_routes_price trp ON trp.component_id = c.id
        AND trp.id = fso.route_id  -- Only get the transport route selected by the student
    WHERE s.id = $P{studentID} AND t.id = $P{termID}
),
term_total AS (
    SELECT SUM(
        CASE
            WHEN component_description = 'Transport' AND transport_price IS NOT NULL
            THEN transport_price
            ELSE component_amount
        END
    ) AS total_amount
    FROM fee_data
)
SELECT * FROM (
    -- Detailed fee component rows
    SELECT
        fee_structure_id,
        fee_structure_name,
        fee_structure_total,
        fee_structure_year,
        component_id,
        -- Use transport price if available, otherwise use component amount
        CASE
            WHEN component_description = 'Transport' AND transport_price IS NOT NULL
            THEN transport_price
            ELSE component_amount
        END AS component_amount,
        component_deadline,
        component_description,
        component_fee_status,
        term_id,
        term_name,
        grade_id,
        grade_name,
        student_id,
        student_first_name,
        student_last_name,
        fso_id,
        fso_created_by,
        fso_parent_id,
        fso_route_id,
        transport_price_id,
        transport_route,
        transport_price,
        transport_route_option,
        is_total_row,
        total_label
    FROM fee_data

    UNION ALL

    -- Total row
    SELECT
        NULL AS fee_structure_id,
        NULL AS fee_structure_name,
        NULL AS fee_structure_total,
        NULL AS fee_structure_year,
        NULL AS component_id,
        total_amount AS component_amount,
        NULL AS component_deadline,
        NULL AS component_description,
        NULL AS component_fee_status,
        NULL AS term_id,
        NULL AS term_name,
        NULL AS grade_id,
        NULL AS grade_name,
        NULL AS student_id,
        NULL AS student_first_name,
        NULL AS student_last_name,
        NULL AS fso_id,
        NULL AS fso_created_by,
        NULL AS fso_parent_id,
        NULL AS fso_route_id,
        NULL AS transport_price_id,
        NULL AS transport_route,
        NULL AS transport_price,
        NULL AS transport_route_option,
        1 AS is_total_row,
        'TERM TOTAL' AS total_label
    FROM term_total
) AS result
ORDER BY
    CASE WHEN is_total_row IS NULL THEN 0 ELSE is_total_row END,
    component_description;]]>
    </queryString>

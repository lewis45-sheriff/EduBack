	<queryString>
		<![CDATA[
SELECT
    s.id AS sId,
    s.admission_number AS sAdmNo,
    s.first_name + ' ' + s.last_name AS sName,
    u.id AS pId,
    u.first_name + ' ' + u.last_name AS pName,
    sph.invoice_number AS invoiceNumber,
    sph.date AS invoiceDate,
    fP.payment_mode AS paymentMode,
    fP.payment_mode + ' ' + fP.receipt_number AS receiptNumber,
    s.first_name + ' ' + s.last_name + ' (' + s.admission_number + ')' AS paymentFor,
    t.id AS tId,
    t.term_name + ' ' + CAST(YEAR(t.start_month) AS VARCHAR) AS tName,
    g.name AS gName,
    CAST(th.transaction_date AS DATE) AS thDate,
    FORMAT(SUM(th.payment_amount) OVER (PARTITION BY t.id, s.id, th.grade_id), 'N2') AS totalPaidTerm,
    FORMAT(sph.term_total_fee_amount, 'N2') AS totalTermFee,
    th.payment_amount AS thPaid,
    FORMAT(sph.term_total_fee_amount - sph.term_total_fee_paid, 'N2') AS totalTermBalance,
    FORMAT(th.balance_after, 'N2') AS balanceAfter,
    sph.*,
    th.*,
    fP.*
FROM transaction_history th
JOIN student s ON s.id = th.student_id
JOIN term t ON th.term = t.id
JOIN [user1] u ON s.parent_id = u.id
JOIN fee_payments fP ON fP.receipt_number = th.receipt_number
JOIN grade g ON g.id = th.grade_id
JOIN student_payment_history sph
    ON sph.student_id = s.id
    AND sph.grade_id = th.grade_id
    AND sph.term_id = th.term
WHERE u.id =$P{parentId}
ORDER BY th.term, s.id, th.transaction_date;

]]>

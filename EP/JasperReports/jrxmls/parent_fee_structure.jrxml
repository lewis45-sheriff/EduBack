	<queryString language="SQL">
		<![CDATA[SELECT
    s.id AS sId,
    s.admission_number AS admNo,
    s.is_new_student,
    s.grade_name AS gName,
    CONCAT(s.first_name, ' ', s.last_name) AS studentFullName,
    stream.stream_name AS studentStream,
    CONCAT(p.first_name, ' ', p.last_name) AS parentFullName,
    CONCAT(s.grade_name, ' ', COALESCE(stream.stream_name, '')) AS fullGradeName,
    fs.id AS feeStructureId,
    fs.name AS feeStructureName,
    fs.year AS feeStructureYear,
    fsComponent.id AS fsComponentId,

    -- Correct FORMAT usage with casting
    FORMAT(
        CAST(
            CASE
                WHEN trPRICE.price IS NOT NULL THEN trPRICE.price
                ELSE fsComponent.amount
            END AS DECIMAL(10,2)
        ), 'N2'
    ) AS fsComponentAmount,

    CAST(fsComponent.deadline AS DATE) AS fsComponentDeadline,
    fsComponent.description AS fsComponentDesc,
    fsComponent.fee_status AS fsComponentStatus,
    fsComponent.fee_structure_id AS fsComponentFeeStructureId,
    fsComponent.term_id AS fsComponentTermId,
    t.term_name AS termName,

    -- Total per student
    FORMAT(
        SUM(
            CAST(
                CASE
                    WHEN trPRICE.price IS NOT NULL THEN trPRICE.price
                    ELSE fsComponent.amount
                END AS DECIMAL(10,2)
            )
        ) OVER(PARTITION BY s.admission_number),
        'N2'
    ) AS TotalStudentFee,

    -- Total across all students
    FORMAT(
        SUM(
            CAST(
                CASE
                    WHEN trPRICE.price IS NOT NULL THEN trPRICE.price
                    ELSE fsComponent.amount
                END AS DECIMAL(10,2)
            )
        ) OVER(),
        'N2'
    ) AS totalFeeForAllStudents,

    trPRICE.price AS optionalCost,
    fsOptionals.*
FROM
    student s
JOIN
    [user] p ON s.parent_id = p.id
JOIN
    fee_structure fs ON s.grade_id = fs.grade_id
JOIN
    component fsComponent ON fs.id = fsComponent.fee_structure_id
JOIN
    term t ON t.id = fsComponent.term_id
LEFT JOIN
    fee_structure_selected_optionals fsOptionals ON s.id = fsOptionals.student_id
    AND fsComponent.id = fsOptionals.component_id
LEFT JOIN
    stream ON stream.id = s.stream_id
LEFT JOIN
    transport_routes_price trPRICE ON trPRICE.id = fsOptionals.route_id
WHERE
    s.completed_last_grade = 'N'
    AND s.parent_id = $P{parentId}
    AND fsComponent.term_id = $P{termID}
    AND (
        fsComponent.fee_status != 'Optional'
        OR (fsComponent.fee_status = 'Optional' AND trPRICE.price > 0)
    )
    AND NOT (
        s.is_new_student = 'N' AND fsComponent.fee_status = 'New_Admisssion'
    )
ORDER BY
    s.admission_number, fsComponent.term_id, fsComponent.id;]]>
